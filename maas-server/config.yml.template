listen:
  addr: <ADDR TO LISTEN ON> 
  port: 8443
ssl:
  key: server.key.pem
  cert: server.cert.pem
auth:
  hash:
    rounds: 10
jwt:
  issuer: <TOKEN ISSUER>
  sig:
    secret: <HASH SALT>
  valid_sec: <JWT TOKEN VALIDITY IN SECONDS>
db:
  sqlite3:
    items: ./db/matomat.db
    users: ./db/matomat.db
    items_stats: ./db/matomat.db